---
title: Geuze
metadescription: The summary in search results
---
<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>{{ title }}</title>
        <meta name="description" content="{{ metadescription }}">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% set css %}
        :root {
        {% for name, color in theming.colors %}
        --{{ name }}: {{ color }};
        {% endfor %}
        {% for name, font in theming.fonts %}
        --{{ name }}: {{ font | safe }};
        {% endfor %}
        }
        {% include "../data/pages/css/imports/layout.css" %}
        {% include "../data/pages/css/imports/header.css" %}
        {% endset %}
        <style>{{ css | cssmin | safe }}</style>
        <link rel="preload" href="/css/fonts.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
        <noscript><link rel="stylesheet" href="/css/fonts.css"></noscript>
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link rel="preload" href="{{ theming.googlefonturl }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
        <noscript><link rel="stylesheet" href="{{ theming.googlefonturl }}"></noscript>
    </head>
    <body>
        <header>
            <a href="/">
                <img src="{{ header.logo }}" alt="logo" width="128" height="64" />
            </a>
            <nav>
                <ul>{% for link in header.links %}<li><a href="{{ link.url }}"{% if link.url == page.url %} active="true"{% endif %}>{{ link.title }}</a></li>{% endfor %}</ul>
            </nav>
        </header>
        <nav>
            <ul>{% for link in nav.links %}<li><a href="{{ link.url }}"{% if link.url == page.url %} active="true"{% endif %}>{{ link.title }}</a></li>{% endfor %}</ul>
        </nav>
        <main>
            {{ content | safe }}
        </main>
        <link rel="stylesheet" href="/css/footer.css">
        <footer>
            <div pancake="deconstructed">
                {% if footer.socialmedialinks.length %}
                <div grow="false">
                    {% markdown %}{% include "../data/config/socialmediaintro.md" %}{% endmarkdown %}
                    <nav>
                        <ul>{% for link in footer.socialmedialinks %}<li><a href="{{ link.url }}" rel="me" title="{{ link.title }}"><span icon="{{ icons[link.icon] | safe }}"></span></a></li>{% endfor %}</ul>
                    </nav>
                </div>
                {% endif %}
                <div>{% markdown %}{% include "../data/config/footercontent.md" %}{% endmarkdown %}</div>
            </div>
            <div>
                <nav>
                    <ul>{% for link in footer.bottomlinks %}<li><a href="{{ link.url }}"{% if link.url == page.url %} active="true"{% endif %}>{{ link.title }}</a></li>{% endfor %}</ul>
                </nav>
                <small>{{ footer.copyright }}</small>
            </div>
        </footer>
        <a href="javascript:(e=>document.querySelector('body').setAttribute('menu', document.querySelector('body').getAttribute('menu') !== 'true'))()" title="Menu"></a>
        <script type="module" src="/js/styles.js"></script>
        <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
        <script>
            if (window.netlifyIdentity) {
                window.netlifyIdentity.on("init", user => {
                    if (!user) {
                        window.netlifyIdentity.on("login", () => {
                            document.location.href = "/admin/";
                        });
                    }
                });
            }
        </script>
    </body>
</html>
